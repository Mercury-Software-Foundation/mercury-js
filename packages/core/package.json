{
  "name": "@mercury-js/core",
  "description": "Fully automated GrpahQL API generator for your mongoDB models",
  "private": false,
  "version": "2.0.0-beta.9",
  "main": "dist/cjs/mercury.js",
  "module": "dist/esm/mercury.js",
  "types": "dist/esm/src/mercury.d.ts",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "dev": "rollup -c -w",
    "test": "jest --watch"
  },
  "exports": {
    ".": {
      "require": "./dist/esm/mercury.js",
      "import": "./dist/esm/mercury.js",
      "types": "./dist/esm/src/mercury.d.ts"
    },
    "./packages/redisCache": {
      "require": "./dist/cjs/packages/redisCache/index.js",
      "import": "./dist/esm/packages/redisCache/index.js",
      "types": "./dist/esm/src/packages/redisCache/index.d.ts"
    },
    "./packages/rateLimiter": {
      "require": "./dist/cjs/packages/rateLimiter/index.js",
      "import": "./dist/esm/packages/rateLimiter/index.js",
      "types": "./dist/esm/src/packages/rateLimiter/index.d.ts"
    },
    "./packages/trpcServer": {
      "require": "./dist/cjs/packages/trpcServer/index.js",
      "import": "./dist/esm/packages/trpcServer/index.js",
      "types": "./dist/esm/src/packages/trpcServer/index.d.ts"
    }
  },
  "files": [
    "dist",
    "packages"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "mongoose",
    "realmoose",
    "headless crm",
    "crm",
    "apollo graphql",
    "graphql"
  ],
  "author": "Roshan K Gujarathi <roshankumargujarathi@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/roshanreacts/mercury-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roshanreacts/mercury-js.git"
  },
  "bugs": {
    "url": "https://github.com/roshanreacts/mercury-js/issues"
  },
  "dependencies": {
    "@graphql-tools/merge": "^8.3.18",
    "bcryptjs": "^2.4.3",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.20.1",
    "kareem": "^2.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^8.0.1",
    "mongoose-paginate-v2": "^1.7.1",
    "nconf": "^0.12.0",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/graphql-fields": "^1.3.4",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^29.7.0",
    "pinst": "^2.1.6",
    "prettier": "^2.4.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "majestic": {
    "jestScriptPath": "../node_modules/jest/bin/jest.js",
    "args": [
      "--config=./jest.config.cjs"
    ],
    "env": {
      "CI": "true"
    }
  }
}
